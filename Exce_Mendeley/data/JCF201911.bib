keywords = {3D topographic reconstruction,Coral,Coral ecology,Coral reef,Habitat,Photogrammetry,Reef structural complexity,Structure-from-motion},
pages = {e1077},
pmid = {26207190},
title = {{Integrating structure-from-motion photogrammetry with geospatial software as a novel technique for quantifying 3D ecological characteristics of coral reefs}},
url = {https://doi.org/10.7717/peerj.1077{\%}5Cnhttps://www.ncbi.nlm.nih.gov/pmc/articles/PMC4511817/pdf/peerj-03-1077.pdf},
