isbn = {9781943436064},
journal = {Proceedings of the 32nd International Conference on Computers and Their Applications, CATA 2017},
keywords = {3D reconstruction,Archaeological site,Architecture,Autonomou,Autonomous underwater vehicles,Hist,Three dimensional computer graphics},
number = {1},
pages = {211--216},
title = {{Pipeline for reconstruction and visualization of underwater archaeology sites using photogrammetry}},
