title = {{The Navy Unmanned Undersea Vehicle (UUV) Master Plan}},
url = {http://www.navy.mil/navydata/technology/uuvmp.pdf},
year = {2004}
}
@article{Yan2016,
abstract = {In order to achieve a smooth and non-overshoot performance for risk of striking the seabed in UUV bottom-sitting task, a soft landing guidance law is presented, which is inspired from the corresponding concept in aerospace science. The guidance law is a polynomial function with respect to time, and the desired vertical position, velocity and acceleration of UUV are determined. In particular, the necessary and sufficient condition for rationality of the guidance law is explained and proved. Theoretical analysis indicates that, with this method, the energy consumption of UUV only depends on the initial velocity and the final velocity, and thus a trade-off between time consumption and energy consumption could be achieved when setting desired velocities. Simulation results show that the UUV moves towards the destination at the determined velocity and acceleration by the proposed guidance law without overshoot, and the acceleration approaches zero according with the concept of soft landing. Safety, rationality, and effectiveness of the proposed guidance law are demonstrated.},
author = {Yan, Zheping and Zhang, Gengshi and Li, Benyin and Chen, Tao},
doi = {10.1109/ChiCC.2016.7554193},
file = {:media/documentos/Drive USB/Publicaciones/2016/2016{\_}Yan et al.{\_}Soft landing guidance law for UUV bottom-sitting task.pdf:pdf},
isbn = {9789881563910},
issn = {21612927},
journal = {Chinese Control Conference, CCC},
keywords = {UUV,bottom-sitting,guidance,soft landing},
pages = {5380--5385},
title = {{Soft landing guidance law for UUV bottom-sitting task}},
volume = {2016-Augus},
year = {2016}
}
@article{Zhang2013b,
abstract = {Autonomous underwater vehicle (AUV) has become effective equipment in exploring and researching oceanic resources. Due to the limitation of the loading capacity of energy, the working ability of AUV is restricted. In this paper, we optimized the structure and hydrodynamics of the landing AUV II to improve the endurance. By decreasing diameter and increasing length, the resistance of the vehicle can be decreased with the same displacement. Although this may sacrifice the maneuverability, the vehicle is verified to have approving attitude controllability. In order to reduce landing impact, the AUV is conducted to dive to the predetermined depth close to seabed via controlling the attitude and then open water intake valves to get weight and land on sea bottom. Moreover, with the establishment of the dynamic model, the maneuverability and dynamic stability is analyzed and the equilibrium rudder angle is calculated. Sea trial indicates that the AUV could fulfill bottom-sitting and depth-setting exploration successfully. {\textcopyright} (2013) Trans Tech Publications, Switzerland.},
author = {Zhang, Hong Wei and Hao, Liang},
doi = {10.4028/www.scientific.net/AMR.694-697.1641},
file = {:media/documentos/Drive USB/Publicaciones/2013/2013{\_}Zhang, Hao{\_}Structure and hydrodynamics optimizations of landing autonomous underwater vehicle.pdf:pdf},
isbn = {9783037856932},
issn = {10226680},
journal = {Advanced Materials Research},
keywords = {Autonomous underwater vehicle,Bottom-sitting,Dynamic stability,Equilibrium rudder angle},
pages = {1641--1645},
title = {{Structure and hydrodynamics optimizations of landing autonomous underwater vehicle}},
volume = {694 697},
year = {2013}
}
@article{Chen2016,
abstract = {Hadal science is one of research frontiers in earth science. Exploring the hadal zone has become an attractive and challenging event in marine engineering due to extremely hostile environment. Only few underwater vehicles have the ability to dive deeper than 6000 meters. Free-fall vehicle, also known as lander has been widely applied in marine investigation for decades, and has made a spectacular achievement in hadal research. This paper presents the sea trial and corresponding results of a 7000-meter lander, which mainly focused on biology and ecology research in hadal zone. The sea trial was conducted in shallow water area and deep-sea area in South China Sea, where functional tests, descent/ascent speed characteristics, and comprehensive performance were studied. During the deep-sea trial, a serial of scientific research achievements was obtained. Sea trial results validated the multi-function, effectiveness and stability of the lander system.},
author = {Chen, Jun and Zhang, Qifeng and Zhang, Aiqun and He, Lisheng and Chen, Qi},
doi = {10.23919/oceans.2015.7401896},
file = {:media/documentos/Drive USB/Publicaciones/2016/2016{\_}Chen et al.{\_}Sea trial and free-fall hydrodynamic research of a 7000-meter lander.pdf:pdf},
isbn = {9780933957435},
journal = {OCEANS 2015 - MTS/IEEE Washington},
keywords = {deep-sea fauna,free-fall hydrodynamic,free-fall vehicle,funnel trap,imaging,lander,sea trial,water sampling},
pages = {1--5},
publisher = {MTS},
title = {{Sea trial and free-fall hydrodynamic research of a 7000-meter lander}},
year = {2016}
}
@article{Yamaguchi2017,
abstract = {An autonomous underwater glider which equips an OBEM (Ocean Bottom Electromagnetometer) for the ocean bottom resources explorations is proposed. The vehicle is composed of a main wing for gliding and tail wings for motion control of the vehicle. Buoyancy and center of gravity control systems, a buttery, sensors and electric devices are contained in water tight vessels under the main wing. This autonomous vehicle has an ability to achieve a long term continuous resources exploration using OBEM. The buoyancy and attitude control mechanism enables the vehicle to move to the next measurement point by gliding. The landing point for the measurement is precisely controlled by the motion control system. In this report, numerical calculations based on CFD are carried out to design a suitable main wing shape and these results are confirmed in comparison with tank experiments. The gliding performance of the vehicle which has a blended wing shape is discussed based on the calculations and the development of a prototype vehicle is also shown.},
author = {Yamaguchi, Satoru and Hirofumi, Sumoto and Katsu, Taishiro and Kono, Yasuki},
file = {:media/documentos/Drive USB/Publicaciones/2017/2017{\_}Yamaguchi et al.{\_}Development of an autonomous OBEM measurement vehicle for offshore resources exploration.pdf:pdf},
