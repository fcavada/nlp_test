@article{Toledo-Hernandez2014,
abstract = {The lionfish (Pterois volitans), has become an abundant alien species along the southeast coast of the United States of America and in Caribbean coastal waters. Although, they represent a major concern in conservation, no studies have yet assessed their population ecology and genetic structure simultaneously. We collected 227 lionfish from 24 sites around Puerto Rico to investigate, the length distribution, length-weight relationships, relative condition factor, and mitochondrial DNA (mtDNA) genetic diversity. The total length distribution ranged between 37 and 349 mm with a single peak between 130 and 149 mm. Our analysis indicated isometric growth for mature individuals and a positive allometric growth for immature lionfishes. These results suggest a different allocation of resources, most likely reproduction for mature individuals and somatic growth for the immature lionfish. The relative condition factor (KN) was similar for immature and mature individuals. Genetic analyses indicated that P. volitans is the only lionfish species present in Puerto Rico, and that its origin was likely dispersal from the epicenter of introduction off the coast of Florida. Interestingly, only four of the nine mtDNA haplotypes identified in the Atlantic were observed in the Puerto Rican population. A fine-scale population genetic analysis suggested a significantly different population structure between the west and south coasts. Finally, our data suggests a second founder effect, but with a reduction of haplotype diversity. {\textcopyright} 2014 The Author(s).},
author = {Toledo-Hern{\'{a}}ndez, Carlos and V{\'{e}}lez-Zuazo, Ximena and Ruiz-Diaz, Claudia Patricia and Patricio, Ana Rita and Mege, Pascal and Navarro, Melissa and Sabat, Alberto Miguel and Betancur-R, Ricardo and Papa, Riccardo},
doi = {10.3391/ai.2014.9.2.12},
file = {:media/documentos/Drive USB/Publicaciones/2014/2014{\_}Toledo-Hern{\'{a}}ndez et al.{\_}Population ecology and genetics of the invasive lionfish in Puerto Rico.pdf:pdf},
issn = {18185487},
journal = {Aquatic Invasions},
keywords = {COI,Caribbean,Invasive reef-fish,MtDNA control region,Pterois spp.,Relative condition factor (KN)},
number = {2},
pages = {227--237},
title = {{Population ecology and genetics of the invasive lionfish in Puerto Rico}},
volume = {9},
year = {2014}
}
@article{Heikkila1997a,
abstract = {In geometrical camera calibration the objective is to determine a$\backslash$nset of camera parameters that describe the mapping between 3-D reference$\backslash$ncoordinates and 2-D image coordinates. Various methods for camera$\backslash$ncalibration can be found from the literature. However surprisingly$\backslash$nlittle attention has been paid to the whole calibration procedure, i.e.,$\backslash$ncontrol point extraction from images, model fitting, image correction,$\backslash$nand errors originating in these stages. The main interest has been in$\backslash$nmodel fitting, although the other stages are also important. In this$\backslash$npaper we present a four-step calibration procedure that is an extension$\backslash$nto the two-step method. There is an additional step to compensate for$\backslash$ndistortion caused by circular features, and a step for correcting the$\backslash$ndistorted image coordinates. The image correction is performed with an$\backslash$nempirical inverse model that accurately compensates for radial and$\backslash$ntangential distortions. Finally, a linear method for solving the$\backslash$nparameters of the inverse model is presented},
