isbn = {0029-8018},
issn = {00298018},
journal = {Ocean Engineering},
keywords = {Autonomous underwater vehicle (AUV),Balance weight parameters,Maneuverability,Motion performance,Motion stability,Seafloor mapping},
pages = {217--225},
publisher = {Elsevier},
title = {{Research on the influence of balance weight parameters on the motion performance of the seafloor mapping AUV in vertical plane}},
