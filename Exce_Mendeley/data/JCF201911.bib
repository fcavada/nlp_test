@inproceedings{Delaunoy2008,
abstract = {Detecting changes between images of the same scene taken at different times is of great interest for monitoring and understanding the environment. It is widely used for on-land application but suffers from different constraints. Unfortunately, Change detection algorithms require highly accurate geometric and photometric registration. This requirement has precluded their use in underwater imagery in the past. In this paper, the change detection techniques available nowadays for on-land application were analyzed and a method to automatically detect the changes in sequences of underwater images is proposed. Target application scenarios are habitat restoration sites, or area monitoring after sudden impacts from hurricanes or ship groundings. The method is based on the creation of a 3D terrain model from one image sequence over an area of interest. This model allows for synthesizing textured views that correspond to the same viewpoints of a second image sequence. The generated views are photometrically matched and corrected against the corresponding frames from the second sequence. Standard change detection techniques are then applied to find areas of difference. Additionally, the paper shows that it is possible to detect false positives, resulting from non-rigid objects, by applying the same change detection method to the first sequence exclusively. The developed method was able to correctly find the changes between two challenging sequences of images from a coral reef taken one year apart and acquired with two different cameras.},
annote = {Deteccion de cambios en imagenes submarinas, con remocion de zonas con posibles cambios debido a reconstruccion 3D.
Primero hacen correccion de las imagenes para eliminar problemas de distorsion y perspectiva. Luego hacen comparacion y deteccion de diferencias.
Finalmente aislan las zonas donde las diferencias se pueden deber a la adquisicion y condiciones, haciendo una match fotometrico. Esto descarta buena parte de los cambios productos de la iluminacion y posibles diferencias en el preprocesamiento de las imagenes.},
author = {Delaunoy, Olivier and Gracias, Nuno Ricardo and Garcia, R.},
booktitle = {OCEANS'08 MTS/IEEE Kobe-Techno-Ocean'08 - Voyage toward the Future, OTO'08},
doi = {10.1109/OCEANSKOBE.2008.4531004},
file = {:media/documentos/Drive USB/Publicaciones/2008/2008{\_}Delaunoy, Gracias, Garcia{\_}Towards detecting changes in underwater image sequences.pdf:pdf},
isbn = {9781424421268},
issn = {0197-7385},
month = {apr},
pages = {1--8},
publisher = {IEEE},
title = {{Towards detecting changes in underwater image sequences}},
year = {2008}
}
@article{Maufroy2010,
abstract = {In this paper, we intend to show the basis of a general legged locomotion controller with the ability to integrate both posture and rhythmic motion controls and shift continuously from one control method to the other according to the walking speed. The rhythmic motion of each leg in the sagittal plane is generated by a single leg controller which controls the swing-to-stance and stance-to-swing phase transitions using respectively leg loading and unloading information. Since rolling motion induced by inverted pendulum motion during the two-legged stance phases results in the transfer of the load between the contralateral legs, leg loading/unloading involves posture information in the frontal plane. As a result of the phase modulations based on leg loading/unloading, rhythmic motion of each leg is achieved and inter-leg coordination (resulting in a gait) emerges, even without explicit coordination amongst the leg controllers, allowing to realize dynamic walking in the low- to medium-speed range. We show that the proposed method has resistance ability against lateral perturbations to some extent, but that an additional ascending coordination mechanism between ipsilateral legs is necessary to withstand perturbations decreasing the rolling motion amplitude. Even without stepping reflex using vestibular information, our control system, relying on phase modulations based on leg loading/unloading and the ascending coordination mechanism between ipsilateral legs, enables low speed dynamic walking on uneven terrain with long cyclic period, which was not realized in our former studies. Details of trajectory generation, movies of simulations and movies of preliminary experiments using a real robot are available at: http://robotics.mech.kit.ac.jp/kotetsu/. {\textcopyright} 2010 Springer Science+Business Media, LLC.},
author = {Maufroy, Christophe and Kimura, Hiroshi and Takase, Kunikatsu},
doi = {10.1007/s10514-009-9172-5},
file = {:media/documentos/Drive USB/Publicaciones/2010/2010{\_}Maufroy, Kimura, Takase{\_}Integration of posture and rhythmic motion controls in quadrupedal dynamic walking using phase modulations.pdf:pdf},
isbn = {1051400991725},
issn = {09295593},
journal = {Autonomous Robots},
keywords = {Central pattern generator,Gait and posture,Leg loading/unloading,Perturbations,Phase modulations,Quadrupedal walking},
month = {feb},
number = {3},
pages = {331--353},
title = {{Integration of posture and rhythmic motion controls in quadrupedal dynamic walking using phase modulations based on leg loading/unloading}},
volume = {28},
year = {2010}
}
@article{Artemiadis2010,
abstract = {Redundant robots have received increased attention during the last$\backslash$ndecades, since they provide solutions to problems investigated for$\backslash$nyears in the robotic community, e.g. task-space tracking, obstacle$\backslash$navoidance etc. However, robot redundancy may arise problems of kinematic$\backslash$ncontrol, since robot joint motion is not uniquely determined. In$\backslash$nthis paper, a biomimetic approach is proposed for solving the problem$\backslash$nof redundancy resolution. First, the kinematics of the human upper$\backslash$nlimb while performing random arm motion are investigated and modeled.$\backslash$nThe dependencies among the human joint angles are described using$\backslash$na Bayesian network. Then, an objective function, built using this$\backslash$nmodel, is used in a closed-loop inverse kinematic algorithm for a$\backslash$nredundant robot arm. Using this algorithm, the robot arm end-effector$\backslash$ncan be positioned in the three dimensional (3D) space using human-like$\backslash$njoint configurations. Through real experiments using an anthropomorphic$\backslash$nrobot arm, it is proved that the proposed algorithm is computationally$\backslash$nfast, while it results to human-like configurations compared to previously$\backslash$nproposed inverse kinematics algorithms. The latter makes the proposed$\backslash$nalgorithm a strong candidate for applications where anthropomorphism$\backslash$nis required, e.g. in humanoids or generally in cases where robotic$\backslash$narms interact with humans.},
annote = {Expresion Cinematica Inversa de un brazo antropomorfico
Menciona Hidden Markov Models
Modelo de dependencias},
author = {Artemiadis, Panagiotis and Katsiaris, Pantelis and Kyriakopoulos, Kostas},
doi = {10.1007/s10514-010-9196-x},
file = {:media/documentos/Drive USB/Publicaciones/2010/2010{\_}Artemiadis, Katsiaris, Kyriakopoulos{\_}A biomimetic approach to inverse kinematics for a redundant robot arm.pdf:pdf},
issn = {09295593},
journal = {Autonomous Robots},
keywords = {Anthropomorphic motion,Biomimetics,Graphical models,Inverse kinematics,Redundant robots},
month = {jun},
number = {3-4},
pages = {293--308},
title = {{A biomimetic approach to inverse kinematics for a redundant robot arm}},
volume = {29},
year = {2010}
}
@article{Bulow2011,
abstract = {3D mapping is very challenging in the underwater domain, especially$\backslash$ndue to the lack of high resolution, low noise sensors. A new spectral$\backslash$nregistration method is presented that can determine the spatial 6$\backslash$nDOF transformation between pairs of very noisy 3D scans with only$\backslash$npartial overlap. The approach is hence suited to cope with sonar$\backslash$nas the predominant underwater sensor. The spectral registration method$\backslash$nis based on Phase Only Matched Filtering (POMF) on non-trivially$\backslash$nresampled spectra of the 3D data. Two extensive sets of experiments$\backslash$nare presented. First, evaluations with simulated data are done where$\backslash$nthe type and amount of noise can be controlled and the ground truth$\backslash$ntransformations between scans are known. Second, real world data$\backslash$nfrom a Tritech Eclipse sonar is used. Concretely, 18 sonar scans$\backslash$nof a large structure in form of a flood gate and a lock in the river$\backslash$nLesum in Bremen are used for 3D mapping. In doing so, the spectral$\backslash$nregistration method is compared to two other methods suited for noisy$\backslash$n3D registrations, namely Iterative Closest Point (ICP) and plane-based$\backslash$nregistration. It is shown that the spectral registration method performs$\backslash$nvery well in terms of the resulting 3D map as well as its run-times.},
author = {B{\"{u}}low, Heiko and Birk, Andreas},
doi = {10.1007/s10514-011-9221-8},
file = {:media/documentos/Drive USB/Publicaciones/2011/2011{\_}B{\"{u}}low, Birk{\_}Spectral registration of noisy sonar data for underwater 3D Mapping.pdf:pdf},
issn = {09295593},
journal = {Autonomous Robots},
keywords = {3D mapping,Autonomous underwater vehicle (AUV),Remotely operated vehicle (ROV),Spectral registration,Underwater robotics},
month = {feb},
number = {3},
pages = {307--331},
title = {{Spectral registration of noisy sonar data for underwater 3D Mapping}},
volume = {30},
year = {2011}
}
@article{Trautmann2011,
abstract = {This paper presents a supervised learning approach to improving the autonomous mobility of wheeled robots through sensing the robot's interaction with terrain ‘underfoot.' Mobility characterization is cast as a hierarchical task, in which pre-immobilization detection is achieved using support vector machines in time to prevent full immobilization, and if a pre-immobilization condition is detected, the associated terrain feature affecting mobility is identified using a Hidden Markov model. These methods are implemented using a hierarchical, layered control scheme developed for the Yeti robot, a 73-kg, four-wheeled robot designed to perform autonomous medium-range missions in polar terrain. The methodology is motivated by the difficultly of visually recognizing terrain features that impact mobility in low contrast terrain. The efficacy of the approach is evaluated using data from a suite of proprioceptive sensors. Real-time implementation shows that Yeti can consistently detect pre-immobilization conditions, stop in time to avoid unrecoverable immobilization, identify the terrain feature presenting the mobility challenge, and execute an escape sequence to retreat from the condition.},
author = {Trautmann, Eric and Ray, Laura},
doi = {10.1007/s10514-011-9224-5},
file = {:media/documentos/Drive USB/Publicaciones/2011/2011{\_}Trautmann, Ray{\_}Mobility characterization for autonomous mobile robots using machine learning.pdf:pdf},
issn = {09295593},
journal = {Autonomous Robots},
keywords = {Autonomous system mobility,Machine learning,Polar robot,Proprioception},
month = {mar},
number = {4},
pages = {369--383},
title = {{Mobility characterization for autonomous mobile robots using machine learning}},
volume = {30},
year = {2011}
}
@article{Karras2011,
abstract = {In this paper we propose a framework for semi-autonomous operation of an under-actuated underwater vehicle. The contributions of this paper are twofold: The first contribution is a visual servoing control scheme that is designed to provide a human operator the capability to steer the vehicle without loosing the target from the vision system's field of view. It is shown that the under-actuated degree of freedom is input-to-state stable (ISS) and a shaping of the user input with stability guarantees is implemented. The resulting control scheme has formally guaranteed stability and convergence properties. The second contribution is an asynchronous Modified Dual Unscented Kalman Filter (MDUKF) for the on-line state and parameter estimation of the vehicle by fusing data from a Laser Vision System (LVS) and an Inertial Measurement Unit (IMU). The MDUKF has been developed in order to experimentally verify the performance of the proposed visual servoing control scheme. Experimental results of the visual servoing control scheme integrated with the asynchronous MDUKF indicate the feasibility and applicability of the proposed control scheme. Experiments have been carried out on a small under-actuated Remotely Operated Vehicle (ROV) in a test tank.},
author = {Karras, George C. and Loizou, Savvas G. and Kyriakopoulos, Kostas J.},
doi = {10.1007/s10514-011-9231-6},
file = {:media/documentos/Drive USB/Publicaciones/2011/2011{\_}Karras, Loizou, Kyriakopoulos{\_}Towards semi-autonomous operation of under-actuated underwater vehicles Sensor fusion, on-line identi.pdf:pdf},
isbn = {0929-5593},
issn = {09295593},
journal = {Autonomous Robots},
keywords = {Parameter estimation,Sensor fusion,Under-actuation,Underwater vehicle,Visual servoing},
month = {may},
number = {1},
pages = {67--86},
title = {{Towards semi-autonomous operation of under-actuated underwater vehicles: Sensor fusion, on-line identification and visual servo control}},
volume = {31},
year = {2011}
}
@inproceedings{Vasilescu2011,
abstract = {Capturing color in water is challenging due to the heavy non-uniform attenuation of light in water across the visible spectrum, which results in dramatic hue shifts toward blue. Yet observing color in water is important for monitoring and surveillance as well as marine biology studies related to species identification, individual and group behavior, and ecosystem health and activity monitoring. Underwater robots are equipped with motor control for large scale transects but they lack sensors that enable capturing color-accurate underwater images. We present a method for color-accurate imaging in water called perceptual adaptive illumination. This method dynamically mixes the illumination of an object in a distance-dependent way using a controllable, multi-color light source. The color mix compensates correctly for color loss and results in an image whose color composition is equivalent to rendering the object in air. Experiments were conducted with a color palette in the pool and at three different coral reefs sites, and with an underwater robot collecting image data with the new sensor.},
author = {Vasilescu, Iuliu and Detweiler, Carrick and Rus, Daniela},
booktitle = {Autonomous Robots},
doi = {10.1007/s10514-011-9245-0},
file = {:media/documentos/Drive USB/Publicaciones/2011/2011{\_}Vasilescu, Detweiler, Rus{\_}Color-accurate underwater imaging using perceptual adaptive illumination.pdf:pdf},
isbn = {0929-5593},
issn = {09295593},
keywords = {Accurate colors,Adaptive illumination,Color rendering index,Underwater imaging},
month = {aug},
number = {2-3},
pages = {285--296},
title = {{Color-accurate underwater imaging using perceptual adaptive illumination}},
volume = {31},
year = {2011}
}
