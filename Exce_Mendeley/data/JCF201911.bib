}
@article{Paduan2009,
abstract = {The Monterey Bay Aquarium Research Institute (MBARI) has developed an autonomous underwater vehicle (AUV) for high resolution seafloor mapping. This vehicle is equipped with a 200 kHz multibeam sonar, 110/410 kHz sidescan sonar, and a 2-16 kHz subbottom profiler. From a 50 m altitude, the AUV achieves 1-meter lateral and 0.1-meter vertical bathymetric resolution and {\textless}1-meter resolution sidescan imagery. The subbottom profiler images subsurface structure at 0.1-meter resolution with up to 60-m penetration. Applications to geohazard mapping include mapping of recently active faults, slumps, submarine canyons, and volcanic terrains. Repeated surveys allow detection of morphological changes associated with active processes.},
author = {Paduan, Jennifer B. and Caress, David W. and Clague, David A. and Paull, Charles K. and Thomas, Hans},
