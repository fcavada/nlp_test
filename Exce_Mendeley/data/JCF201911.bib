journal = {Proceedings of the Seventh IEEE International Conference on Computer Vision},
number = {[8},
pages = {1150--1157},
pmid = {15806121},
primaryClass = {cs},
publisher = {IEEE},
title = {{Object recognition from local scale-invariant features}},
