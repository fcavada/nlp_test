file = {:media/documentos/Drive USB/Publicaciones/2018/2018{\_}He et al.{\_}A Twofold Siamese Network for Real-Time Object Tracking.pdf:pdf},
isbn = {9781538664209},
issn = {10636919},
journal = {Proceedings of the IEEE Computer Society Conference on Computer Vision and Pattern Recognition},
