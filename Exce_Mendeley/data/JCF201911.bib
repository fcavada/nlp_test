journal = {IFAC-PapersOnLine},
keywords = {AMVs,Input constraint,MPC,Path following},
number = {29},
pages = {348--353},
publisher = {Elsevier B.V.},
title = {{Input-Constrained Path Following for Autonomous Marine Vehicles with a Global Region of Attraction ⁎}},
url = {https://doi.org/10.1016/j.ifacol.2018.09.499},
volume = {51},
year = {2018}
}
@article{DePalma2018,
abstract = {This paper describes two navigation filters designed for an Autonomous Underwater Vehicle (AUV) for geotechnical surveying. Both a Luenberger observer for a kinematic model of the vehicle as well as an extended Kalman filter for its dynamic model are addressed. The filters allow to fuse information coming from a Global Positioning System (GPS), a compass, a gyro, a depth meter, and acoustic based range measurements. A thruster model mapping low level actuator commands to vehicle surge velocity is also exploited in the design. The performances of both filters have been compared using the experimental data collected during the H2020 WiMUST (Widely scalable Mobile Underwater Sonar Technology) project experiments.},
author = {{De Palma}, Daniela and Indiveri, Giovanni},
doi = {10.1016/j.ifacol.2018.09.488},
