@article{Cao1997,
abstract = {There has been increased research interest in systems composed of multiple autonomous mobile $\backslash$nrobots exhibiting cooperative behavior. Groups of mobile robots are constructed, with an aim to studying $\backslash$nsuch issues as group architecture, resource conflict, origin of cooperation, learning, and geometric prob- $\backslash$nlems. As yet, few applications of cooperative robotics have been reported, and supporting theory is still in $\backslash$nits formative stages. In this paper, we give a critical survey of existing works and discuss open problems $\backslash$nin this field, emphasizing the various theoretical issues that arise in the study of cooperative robotics. We $\backslash$ndescribe the intellectual heritages that have guided early research, as well as possible additions to the set $\backslash$nof existing motivations.},
