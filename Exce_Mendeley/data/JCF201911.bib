isbn = {0-7803-5045-6},
keywords = {Shi-Tomasi-Kanade scheme,X84,feature extraction,fundamental matrix estimation,image sequences,oceanographic techniques,outlier rejection rule,outlier rejection scheme,robust feature tracker,robust feature tracking,spurious features,subsea video sequences,theoretical assumptions,tracking,underwater video sequences,unreliable features,video signal processing},
number = {1},
