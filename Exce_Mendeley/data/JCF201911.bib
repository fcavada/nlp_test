title = {{Coordinated Navigation Using Dynamically Varying Velocity Fields}},
year = {2007}
}
@article{Buchli2004,
abstract = {A method is presented to predict phase relationships between coupled$\backslash$nphase oscillators. As an illustration of how the method can be applied,$\backslash$na distributed Central Pattern Generator (CPG) model based on amplitude$\backslash$ncontrolled phase oscillators is presented. Representative results$\backslash$nof numerical integration of the CPG model are presented to illustrate$\backslash$nits excellent properties in terms of transition speeds, robustness$\backslash$nand independence on initial conditions. A particularly interesting$\backslash$nfeature of the CPG is the possibility to switch between different$\backslash$nstable gaits by varying a single parameter. These characteristics$\backslash$nmake the CPG model an interesting solution for the decentralized$\backslash$ncontrol of multi-legged robots. The approach is discussed in the$\backslash$nmore general framework of coupled nonlinear systems, and design tools$\backslash$nfor nonlinear distributed control schemes applicable to Information$\backslash$ntechnology and robotics.},
