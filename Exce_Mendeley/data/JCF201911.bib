keywords = {Amplitude controlled phase oscillators,Central pattern generator,Force feedback,Legged robots,Machine design,Mobile robots,Phase modulation,Phase oscillators,Static stability},
pages = {37--46},
title = {{A CPG with force feedback for a statically stable quadruped gait}},
