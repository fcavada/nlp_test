@article{Nicosevici2009,
abstract = {Scene modeling has a key role in applications ranging from visual$\backslash$nmapping to augmented reality. This paper presents an end-to-end solution$\backslash$nfor creating accurate three-dimensional (3D) textured models using$\backslash$nmonocular video sequences. The methods are developed within the$\backslash$nframework of sequential structure from motion, in which a 3D model of$\backslash$nthe environment is maintained and updated as new visual information$\backslash$nbecomes available. The proposed approach contains contributions at$\backslash$ndifferent levels. The camera pose is recovered by directly associating$\backslash$nthe 3D scene model with local image observations, using a$\backslash$ndual-registration approach. Compared to the standard structure from$\backslash$nmotion techniques, this approach decreases the error accumulation while$\backslash$nincreasing the robustness to scene occlusions and feature association$\backslash$nfailures, while allowing 3D reconstructions for any type of scene.$\backslash$nMotivated by the need to map large areas, a novel 3D vertex selection$\backslash$nmechanism is proposed, which takes into account the geometry of the$\backslash$nscene. Vertices are selected not only to have high reconstruction$\backslash$naccuracy but also to be representative of the local shape of the scene.$\backslash$nThis results in a reduction in the complexity of the final 3D model,$\backslash$nwith minimal loss of precision. As a final step, a composite visual map$\backslash$nof the scene (mosaic) is generated. We present a method for blending$\backslash$nimage textures using 3D geometric information and photometric$\backslash$ndifferences between registered textures. The method allows high-quality$\backslash$nmosaicing over 3D surfaces by reducing the effects of the distortions$\backslash$ninduced by camera viewpoint and illumination changes. The results are$\backslash$npresented for four scene modeling scenarios, including a comparison with$\backslash$nground truth under a realistic scenario and a challenging underwater$\backslash$ndata set. Although developed primarily for underwater mapping$\backslash$napplications, the methods are general and applicable to other domains,$\backslash$nsuch as aerial and land-based mapping. (C) 2009 Wiley Periodicals, Inc.},
author = {Nicosevici, Tudor and Gracias, Nuno Ricardo and Negahdaripour, Shahriar and Garcia, R.},
doi = {10.1002/rob.20305},
file = {:media/documentos/Drive USB/Publicaciones/2009/2009{\_}Nicosevici et al.{\_}Efficient three-dimensional scene modeling and mosaicing.pdf:pdf},
issn = {15564959},
journal = {Journal of Field Robotics},
month = {oct},
number = {10},
pages = {759--788},
title = {{Efficient three-dimensional scene modeling and mosaicing}},
volume = {26},
year = {2009}
}
@inproceedings{Gracias2008,
abstract = {A common problem in video surveys in very shallow waters is the presence of strong light fluctuations, due to sun light refraction. Refracted sunlight casts fast moving patterns, which can significantly degrade the quality of the acquired data. Motivated by the growing need to improve the quality of shallow water imagery, we propose a method to remove sunlight patterns in video sequences. The method exploits the fact that video sequences allow several observations of the same area of the sea floor, over time. It is based on computing the image difference between a given reference frame and the temporal median of a registered set of neighboring images. A key observation is that this difference will have two components with separable spectral content. One is related to the illumination field (lower spatial frequencies) and the other to the registration error (higher frequencies). The illumination field, recovered by lowpass filtering, is used to correct the reference image. In addition to removing the sunflickering patterns, an important advantage of the approach is the ability to preserve the sharpness in corrected image, even in the presence of registration inaccuracies. The effectiveness of the method is illustrated in image sets acquired under strong camera motion containing non-rigid benthic structures. The results testify the good performance and generality of the approach.},
author = {Gracias, Nuno Ricardo and Negahdaripour, Shahriar and Neumann, Laszlo and Prados, Ricard and Garcia, R.},
booktitle = {OCEANS 2008},
doi = {10.1109/OCEANS.2008.5152111},
file = {:media/documentos/Drive USB/Publicaciones/2008/2008{\_}Gracias et al.{\_}A motion compensated filtering approach to remove sunlight flicker in shallow water images.pdf:pdf},
isbn = {9781424426201},
issn = {0197-7385 ER},
keywords = {image enhancement,lighting artifacts,sunlight flickering,underwater mosaicing},
pages = {1--7},
publisher = {IEEE},
title = {{A motion compensated filtering approach to remove sunlight flicker in shallow water images}},
year = {2008}
}
@inproceedings{Garcia2011,
abstract = {Our research is motivated by an evident lack of evaluation of recent image matching techniques for applications in underwater vision. This paper is a first step in this direction. This work compares the performance of popular salient keypoint detectors on images degraded by turbidity. We show that, as opposed to over-land, on images acquired in water medium, Hessian-based approaches outperform their Laplacian and Harris counterparts. Fast Hessian, the detector of the Speeded Up Robust Features (SURF) matching technique, is recognized to be the best method for scale-invariant detection. Conversely, when invariance to scale is not required, a combination of standard Hessian and Harris with sub-pixel accuracy and non-maxima suppression is more accurate. The objective of our work was also to create and distribute a reference set of turbid images, which can be used to evaluate processing, detection, description and matching techniques for underwater applications. We present a collection of 36 images acquired by a specially designed trinocular system under 12 gradually increasing turbidity levels. We also draw attention to image quality assessment method called SSIM, Structural SIMilarity index, which reliably quantifyes degradation of image quality caused by turbidity. As a whole, the major goal of this paper is to provide an updated reference for researchers dealing with keypoint detection in underwater imaging.},
author = {Garcia, R. and Gracias, Nuno Ricardo},
booktitle = {OCEANS 2011 IEEE - Spain},
doi = {10.1109/Oceans-Spain.2011.6003605},
file = {:media/documentos/Drive USB/Publicaciones/2011/2011{\_}Garcia, Gracias{\_}Detection of interest points in turbid underwater images.pdf:pdf},
isbn = {9781457700866},
issn = {0197-7385},
month = {jun},
pages = {1--9},
publisher = {IEEE},
title = {{Detection of interest points in turbid underwater images}},
year = {2011}
}
@inproceedings{Elibol2009,
abstract = {Large area photo-mosaics are widely used in many different applications such as optical mapping, panorama creation and autonomous vehicle navigation. When the trajectory of the camera provides an overlap between non-consecutive images (closed-loop trajectory), it is essential to detect such events in order to get globally coherent mosaics. Recent advances in image matching methods allow for registering pairs of images in the absence of prior information on orientation, scale or overlap between images. Owing to this, recent batch mosaicing algorithms attempt to detect non-consecutive overlapping images using exhaustive matching of image pairs. This paper proposes the use of Observation Mutual Information as a criterion to evaluate the benefit of potential matches between pairs of images. This allows for ranking and ordering a list of potential matches in order to make the loop-closing process more efficient. In this paper, the Observation Mutual Information criterion is compared against other strategies and results are presented using underwater imagery.},
author = {Elibol, Armagan and Gracias, Nuno Ricardo and Garcia, R.},
booktitle = {Lecture Notes in Computer Science (including subseries Lecture Notes in Artificial Intelligence and Lecture Notes in Bioinformatics)},
doi = {10.1007/978-3-642-02172-5_15},
file = {:media/documentos/Drive USB/Publicaciones/2009/2009{\_}Elibol, Gracias, Garcia{\_}Match selection in batch mosaicing using mutual information.pdf:pdf},
isbn = {3642021719},
issn = {03029743},
pages = {104--111},
title = {{Match selection in batch mosaicing using mutual information}},
volume = {5524 LNCS},
year = {2009}
}
