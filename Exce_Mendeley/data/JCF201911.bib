title = {{Landscape video mosaic from a mesophotic coral reef}},
volume = {29},
year = {2010}
}
@article{Ferre2007,
abstract = {In this paper, stereoscopic image visualization for teleoperated robots is studied. The use of stereo vision systems shows some difficulties: image disparity is the main problem. It affects the correct image fusion process by the brain. Disparity curve calculations are shown and a method to calibrate stereoscopic images for telerobotics is explained. Experiments have been carried out to obtain the limits for a proper image fusion. The use of stereoscopic image has been assessed to execute teleoperated robot guidance tasks. The performance of stereoscopic and monoscopic image is compared. Stereoscopic images have shown a guaranteed depth perception.},
