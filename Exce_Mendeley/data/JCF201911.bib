file = {:media/documentos/Drive USB/Publicaciones/2015/2015{\_}Rende et al.{\_}Pilot application of 3D underwater imaging techniques for mapping Posidonia oceanica (L.) Delile meadows.pdf:pdf},
issn = {16821750},
keywords = {3D mapping,3D reconstruction,Optical instruments,Posidonia oceanica,Quality status},
number = {5W5},
pages = {177--181},
title = {{Pilot application of 3D underwater imaging techniques for mapping Posidonia oceanica (L.) Delile meadows}},
