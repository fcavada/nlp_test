file = {:media/documentos/Drive USB/Publicaciones/2007/2007{\_}Davison et al.{\_}MonoSLAM Real-time single camera SLAM.pdf:pdf},
isbn = {0162-8828},
issn = {01628828},
journal = {IEEE Transactions on Pattern Analysis and Machine Intelligence},
keywords = {3D/stereo scene analysis,Autonomous vehicles,Tracking},
month = {jun},
