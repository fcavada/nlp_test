issn = {24058963},
journal = {IFAC-PapersOnLine},
keywords = {Marine systems,autonomous vehicles,navigation},
number = {29},
pages = {171--176},
publisher = {Elsevier B.V.},
title = {{Navigation filters for Autonomous Underwater Vehicles during geotechnical surveying experiments ⁎}},
