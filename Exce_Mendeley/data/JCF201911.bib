@article{GONZALEZ2003,
abstract = {Leg dynamics are often ignored in the real-time control of walking robots because of the high gearing used in leg transmissions. However, the use of a gear reduction high enough to discount Coriolis and centripetal components yields additional non-desired dynamics, which are friction, backlash and elasticity. In such cases, simplifying robot dynamics without considering the effect of gear dynamics leads to unavoidable errors. In order to make dynamic equations reflect the reality of the physical system, it is of paramount importance to model the most significant effects acting on the system. Robot dynamics could then be analyzed and related to trajectory parameters for motion-control purposes. In this paper, a method to derive the dynamics of a robot leg as a function of leg-trajectory parameters is proposed. The method experimentally finds the simplified equations of motion that reflect the reality of the physical system. The resulting model is an accurate and simple representation of the system dynamics, taking into account the most relevant dynamics affecting the system. The simplification of the model allows it to be used in a real-time dynamic-control system. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
author = {Garcia, Elena and Galvez, J. A. and {Gonzalez De Santos}, Pablo and Gonzalez, Aurelien and Santos, PG De},
doi = {10.1023/A:1026104815610},
file = {:media/documentos/Drive USB/Publicaciones/2003/2003{\_}Garcia et al.{\_}On Finding the Relevant Dynamics for Model-Based Controlling Walking Robots.pdf:pdf},
isbn = {3491871190},
issn = {09210296},
journal = {Journal of Intelligent and Robotic Systems: Theory and Applications},
keywords = {Dynamic modeling,Model-based control,Robot dynamics,Walking robots},
number = {4},
pages = {375--398},
publisher = {万方数据资源系统},
title = {{On Finding the Relevant Dynamics for Model-Based Controlling Walking Robots}},
volume = {37},
year = {2003}
}
@article{Schoerling2009,
abstract = {Experiments with two formation controllers for marine unmanned surface vessels are reported. The formation controllers are designed using the nonlinear robust model-based sliding mode approach. The marine vehicles can operate in arbitrary formation configurations by using two leader-follower control schemes. For the design of these controller schemes 3 degrees of freedom (DOFs) of surge, sway, and yaw are assumed in the planar motion of the marine surface vessels. Each vessel only has two actuators; therefore, the vessels are underactuated and the lack of a kinematic constraint puts them into the holonomic system category. In this work, the position of a control point on the vessel is controlled, and the orientation dynamics is not directly controlled. Therefore, there is a potential for an oscillatory yaw motion to occur. It is shown that the orientation dynamics, as the internal dynamics of this underactuated system, is stable, i.e., the follower vehicle does not oscillate about its control point during the formation maneuvers. The proposed formation controller relies only on the state information obtained from the immediate neighbors of the vessel and the vessel itself. The effectiveness and robustness of formation control laws in the presence of parameter uncertainty and environmental disturbances are demonstrated by using both simulations and field experiments. The experiments were performed in a natural environment on a lake using a small test boat, and show robust performance to parameter uncertainty and disturbance. This paper reports the first experimental verification of the above mentioned approach, whose unique features are the use of a control point, the zero-dynamic stability analysis, the use of leader-follower method and a nonlinear robust control approach.},
annote = {From Duplicate 2 (Experimental test of a robust formation controller for marine unmanned surface vessels - Schoerling, Daniel; Kleeck, Chris; Fahimi, Farbod; Koch, Charles Robert; Ams, Alfons; L{\"{o}}ber, Peter)

Informacion acerca de modelado y control de robots de superficie (ASV).},
author = {Schoerling, Daniel and {Van Kleeck}, Chris and Fahimi, Farbod and Koch, Charles Robert and Ams, Alfons and L{\"{o}}ber, Peter},
doi = {10.1007/s10514-009-9163-6},
file = {:media/documentos/Drive USB/Publicaciones/2010/2010{\_}Schoerling et al.{\_}Experimental test of a robust formation controller for marine unmanned surface vessels.pdf:pdf},
isbn = {0929-5593},
issn = {09295593},
journal = {Autonomous Robots},
keywords = {Autonomous vehicles,Experimental verification,Formation control,Marine vehicles,Sliding mode control,Surface vessels},
month = {dec},
number = {2},
pages = {213--230},
title = {{Experimental test of a robust formation controller for marine unmanned surface vessels}},
url = {http://www.springerlink.com/index/10.1007/s10514-009-9163-6 http://link.springer.com/10.1007/s10514-009-9163-6},
volume = {28},
year = {2010}
}
@article{Garcia2007,
abstract = {Up to now, walking robots have been working outdoors under favorable conditions and using very large stability margins to cope with natural environments and intrinsic robot dynamics that can cause instability in these machines when they use statically-stable gaits. The result has been very slow robots prone to tumble down in the presence of perturbations. This paper proposes a novel gaitadaptation method based on the maximization of the Normalized Dynamic Energy Stability Margin. This method enables walking-machine gaits to adapt to internal (robot dynamics) and external (environmental) perturbations, including the slope of the terrain, by finding the gait parameters that maximize robot stability. The adaptation method is inspired in the natural gait adaptation carried out by humans and animals to balance external forces or the effect of sloping terrain. Experiments with the SILO4 quadruped robot are presented and show how robot stability is more robust when the proposed approach is used for different external forces and sloping terrains. Using the proposed gaitadaptation approach the robot is able to withstand external forces up to 58{\%} the robot weight and 25-degree slopes.},
author = {Garcia, Elena and {Gonzalez De Santos}, Pablo and Matia, F. and {De Santos}, P. Gonzalez},
doi = {10.1007/s10514-007-9079-y},
file = {:media/documentos/Drive USB/Publicaciones/2008/2008{\_}Garcia et al.{\_}Dealing with internal and external perturbations on walking robots.pdf:pdf},
isbn = {0929-5593},
issn = {09295593},
journal = {Autonomous Robots},
keywords = {Autonomous robots,Dynamic stability,Gait adaptation,Robot gaits,Stability margin,Walking robot,autonomous,dynamic,gait adaptation,robot gaits,stability,stability margin,walking robot},
month = {dec},
number = {3},
pages = {213--227},
title = {{Dealing with internal and external perturbations on walking robots}},
volume = {24},
year = {2008}
}
@article{Lopez2008,
abstract = {Cryptofauna associated with colonies of dead coral is important due to their role as bioeroding organisms that alter the physical structure of reefs. We studied dead colonies of Montastraea annularis to obtain information on the main groups present and their relative abundance at Los Roques Archipelago National Park (reefs of Dos Mosquises, Cayo La Pelona, and Cayo Sal). Dead corals were collected at 6-8 m and 9-13 m depth, (3 fragments at each location), and colony volume was estimated for density values standardization. Fragments were broken down in the laboratory and organisms were separated. In all, 1,499 individuals in 8 taxonomic groups were found. The most abundant were sipunculids (57.29{\%}), followed by polychaetes (20.45{\%}), crustaceans (14.76{\%}), nematods, mollusks, and echinoderms. The fauna associated with reefs fragments is highly diverse, but consistently dominated by the same groups.},
author = {L{\'{o}}pez, K and Bone, D. and Rodr{\'{i}}guez, C and Padilla, F.},
file = {:media/documentos/Drive USB/Publicaciones/2008/2008{\_}L{\'{o}}pez et al.{\_}Biodiversity of cryptofauna associated with reefs of the Los Roques Archipelago National Park , Venezuela.pdf:pdf},
journal = {Proceedings of the 11th Int Coral Reef Sym},
keywords = {cryptofauna,dead colonies,montastraea annularis},
number = {26},
pages = {1352--1354},
title = {{Biodiversity of cryptofauna associated with reefs of the Los Roques Archipelago National Park , Venezuela}},
year = {2008}
}
@article{Bonin-Font2008a,
abstract = {Mobile robot vision-based navigation has been the source of countless research contributions, from the domains of both vision and control. Vision is becoming more and more common in applications such as localization, automatic map construction, autonomous navigation, path following, inspection, monitoring or risky situation detection. This survey presents those pieces of work, from the nineties until nowadays, which constitute a wide progress in visual navigation techniques for land, aerial and autonomous underwater vehicles. The paper deals with two major approaches: map-based navigation and mapless navigation. Map-based navigation has been in turn subdivided in metric map-based navigation and topological map-based navigation. Our outline to mapless navigation includes reactive techniques based on qualitative characteristics extraction, appearance-based localization, optical flow, features tracking, plane ground detection/tracking, etc... The recent concept of visual sonar has also been revised.},
