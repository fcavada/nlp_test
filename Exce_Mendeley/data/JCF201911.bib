@article{Eustice2004,
abstract = {This paper describes a framework for sensor fusion of navigation data with camera-based 5 DOF relative pose measurements for 6 DOF vehicle motion in an unstructured 3D underwater environment. The fundamental goal of this work is to concurrently sstimate online current vehicle position and its past trajectory. This goal is framed within the context of improving mobile robot navigation to support sub-sea science and exploration. Vehicle trajectory is represented by a history of poses in an augmented state Kalman filter. Camera spatial constraints from overlapping imagery provide partial observation of these posa and are used to enforce consislency and provide a mechanism for loop-closure. The multi-sensor camera+navigation framework is shown to have compelling advantages over a camera-only based approach by 1) improving the robustness of pairwise image registration, 2) setting the free gauge scale, and 3) allowing for a unconnected camera graph topology. Results are shown for a real world data set collected by an autonomous underwater vehicle in an unstructured undersea environment.},
author = {Eustice, Ryan Michael and Pizarro, Oscar and Singh, Hanumant},
doi = {10.1109/ROBOT.2004.1307124},
file = {:media/documentos/Drive USB/Publicaciones/2004/2004{\_}Eustice, Pizarro, Singh{\_}Visually Augmented Navigation in an Unstructured Environment Using a Delayed State History.pdf:pdf},
isbn = {0780382323},
issn = {1050-4729},
journal = {Proceedings of the IEEE International Conference on Robotics and Automation (ICRA)},
number = {April},
pages = {25--32},
title = {{Visually Augmented Navigation in an Unstructured Environment Using a Delayed State History}},
volume = {1},
year = {2004}
}
@inproceedings{Pizarro2003,
abstract = {Recent efforts in robust estimation of the two-view relation have$\backslash$nfocused on uncalibrated cameras with no prior knowledge of pose.$\backslash$nHowever, in practice robotic vehicles that perform image-based navigation$\backslash$nand mapping typically do carry a calibrated camera and pose sensors;$\backslash$nthis additional knowledge is currently not being exploited.$\backslash$n$\backslash$n$\backslash$nThis paper presents three contributions in using vision with instrumented$\backslash$nand calibrated platforms. First, we improve the performace of the$\backslash$ncorrespondence stage by using uncertain measurements from egomotion$\backslash$nsensors to constrain possible matches. Second, we assume wide-baseline$\backslash$nconditions and propose Zernike moments to describe affine invariant$\backslash$nfeatures. Third, we robustly estimate the essential matrix with a$\backslash$nnew 6-point algorithm. Our solution is simpler than the minimal 5-point$\backslash$none and, unlike the linear 6-point solution, does not fail on planar$\backslash$nscenes. While the contributions are general, we present structure$\backslash$nand motion results from an underwater robotic survey.},
author = {Pizarro, Oscar and Eustice, Ryan Michael and Singh, Hanumant},
booktitle = {Proceedings of Digital Image Computing: Techniques and Applications},
file = {:media/documentos/Drive USB/Publicaciones/2003/2003{\_}Pizarro, Eustice, Singh{\_}Relative pose estimation for instrumented, calibrated imaging platforms.pdf:pdf},
