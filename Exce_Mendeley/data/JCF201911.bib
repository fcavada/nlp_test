doi = {10.5194/isprsarchives-XL-3-W4-127-2016},
file = {:media/documentos/Drive USB/Publicaciones/2016/2016{\_}Nocerino et al.{\_}Underwater calibration of dome port pressure housings.pdf:pdf},
issn = {16821750},
journal = {International Archives of the Photogrammetry, Remote Sensing and Spatial Information Sciences - ISPRS Archives},
keywords = {Camera calibration,Decentring distortion,Photogrammetry,Underwater},
