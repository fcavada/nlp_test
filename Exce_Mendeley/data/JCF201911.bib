file = {:media/documentos/Drive USB/Publicaciones/2012/2012{\_}Friedman et al.{\_}Multi-Scale Measures of Rugosity, Slope and Aspect from Benthic Stereo Image Reconstructions.pdf:pdf},
isbn = {1932-6203},
