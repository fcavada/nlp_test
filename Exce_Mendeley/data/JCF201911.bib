isbn = {0-7803-6551-8},
keywords = {AUV,Acoustic sensors,Biosensors,Instruments,Li battery,Light scattering,Marine technology,Packaging,REMUS,Remote Environmental Monitoring UnitS,Remote monitoring,Remotely operated vehicles,Sea measurements,Underwater vehicles,WHOI,Woods Hole Oceanographic Institution,autonomous underwater vehicle,bioluminescence,capability,chlorophyll fluorometer,circulation,equipment,instrument,marine biology,ocean,oceanographic equipment,optical sensor package,plume mapping,remotely operated vehicles,rhodamine fluorometer,turbulence,underwater light,underwater vehicles},
