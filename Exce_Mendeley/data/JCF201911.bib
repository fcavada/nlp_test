journal = {Journal of Experimental Marine Biology and Ecology},
keywords = {3D modeling,Coral 3D surface area,Coral morphology,Coral reconstruction,Diploria,Hemispheric surrogate,Log-linear model,Montastraea,Photogrammetry,Surface area estimation},
