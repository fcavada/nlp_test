@article{Gonzalez-Rivero2017,
abstract = {Structural complexity strongly influences biodiversity and ecosystem productivity. On coral reefs, structural complexity is typically measured using a single and small-scale metric (‘rugosity') that represents multiple spatial attributes differentially exploited by species, thus limiting a complete understanding of how fish associate with reef structure. We used a novel approach to compare relationships between fishes and previously unavailable components of reef complexity, and contrasted the results against the traditional rugosity index. This study focused on damselfish to explore relationships between fishes and reef structure. Three territorial species, with contrasting trophic habits and expected use of the reef structure, were examined to infer the potential species-specific mechanisms associated with how complexity influences habitat selection. Three-dimensional reef reconstructions from photogrammetry quantified the following metrics of habitat quality: 1) visual exposure to predators and competitors, 2) density of predation refuges and 3) substrate-related food availability. These metrics explained the species distribution better than the traditional measure of rugosity, and each species responded to different complexity components. Given that a critical effect of reef degradation is loss of structure, adopting three-dimensional technologies potentially offers a new tool to both understand species-habitat association and help forecast how fishes will be affected by the flattening of reefs.},
author = {Gonz{\'{a}}lez-Rivero, M. and Harborne, A. R. and Herrera-Reveles, A. and Bozec, Y.-M. and Rogers, Alice and Friedman, A. and Ganase, A. and Hoegh-Guldberg, O.},
doi = {10.1038/s41598-017-14272-5},
file = {:media/documentos/Drive USB/Publicaciones/2017/2017{\_}Gonz{\'{a}}lez-Rivero et al.{\_}Linking fishes to multiple metrics of coral reef structural complexity using three-dimensional technology.pdf:pdf},
isbn = {4159801714},
issn = {2045-2322},
journal = {Scientific Reports},
month = {dec},
number = {1},
pages = {13965},
pmid = {17325230},
title = {{Linking fishes to multiple metrics of coral reef structural complexity using three-dimensional technology}},
url = {http://www.nature.com/articles/s41598-017-14272-5},
volume = {7},
year = {2017}
}
@book{Torrealba2015,
abstract = {{\textcopyright} 2015 by Nova Science Publishers, Inc. All rights reserved. In this chapter, the complete development of an external intelligent knee prosthesiscreated by the Mechatronics Research Group (MRG) of the Sim{\'{o}}n Bol{\'{i}}var University ispresented. This project has brought out several advances in the prosthetic field, especiallyreferring external intelligent knee prostheses, such as bio-inspired shape of the chassis,characterization of gait cycle from accelerometer data captured on the legs duringwalking, continuous control strategies borrowed from nature and robotics, and morerecently, a variable impedance actuator, which allows the MRG prosthesis to go farbeyond the starting expectations. All these were novel features regarding the state-of-theartby the time they were introduced, but of course, all this did not happen at a time; onthe contrary, the project has experienced an evolution from the first passive prototypeequipped with a commercial magneto-rheological (MR) actuator, to the most recentactive one, equipped with a variable stiffness actuator created by us. This chapterexplains most of the details of such evolution process, going through the motivations ofthe project, the technical developments that have emerged along the way, and the flawsand limitations of the approaches tested and how these have been overcome. Somehowthis work is a compilation of a series of articles published by the co-authors on thismatter, but on the other hand, it is a completely new text never published before, as thispresents all the content that has emerged from this project up-to-now as a whole for thefirst time.},
