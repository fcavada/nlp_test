publisher = {Ieee},
title = {{Robust feature tracking in underwater video sequences}},
volume = {1},
year = {1998}
}
@inproceedings{Garcia2001,
abstract = {When underwater vehicles perform navigation close to the ocean floor,$\backslash$ncomputer vision techniques can be applied to obtain quite accurate$\backslash$nmotion estimates. The most crucial step in the vision- based estimation$\backslash$nof the vehicle motion consists on detecting matchings between image$\backslash$npairs. Here we propose the extensive use of texture analysis as a$\backslash$ntool to ameliorate the correspondence problem in underwater images.$\backslash$nOnce a robust set of correspondences has been found, the three-dimensional$\backslash$nmotion of the vehicle can be computed with respect to the bed of$\backslash$nthe sea. Finally, motion estimates allow the construction of a map$\backslash$nthat could aid to the navigation of the robot.},
