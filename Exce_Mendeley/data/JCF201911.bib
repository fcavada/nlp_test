@article{CarrerasM.;Candela2015,
abstract = {This paper describes the experience of preparing and testing the SPARUS$\backslash$r$\backslash$nII AUV in different applications. The AUV was designed as a lightweight vehicle combining$\backslash$r$\backslash$nthe classical torpedo-shape features with the hovering capability. The robot$\backslash$r$\backslash$nhas a payload area to allow the integration of different equipment depending on$\backslash$r$\backslash$nthe devices and systems. Its flexibility, easy operation and openness makes the SPARUS$\backslash$r$\backslash$nII AUV a multipurpose platform that can adapt to industrial, scientific and academic$\backslash$r$\backslash$napplications. Five units were developed in 2014, and different teams used and$\backslash$r$\backslash$nadapted the platform for different applications. The paper describes some of the$\backslash$r$\backslash$nexperiences in preparing and testing this open platform to different applications.},
