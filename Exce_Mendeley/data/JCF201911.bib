issn = {20926790},
journal = {International Journal of Naval Architecture and Ocean Engineering},
keywords = {Computational fluid dynamics,Four-rotor dish-shaped UUV,Hydraulic support,Particle swarm optimization,Response surface model},
number = {5},
pages = {475--486},
publisher = {Elsevier Ltd},
title = {{Optimal design of hydraulic support landing platform for a four-rotor dish-shaped UUV using particle swarm optimization}},
url = {http://dx.doi.org/10.1016/j.ijnaoe.2016.05.007},
