@article{Guinda2013,
abstract = {The assessment of anthropogenic impacts and ecological status of coastal waters is an important task to accomplish under the European Water Framework Directive (WFD 2000/60/EEC). Macroalgae are one of the biological quality elements that must be considered, but their assessment has been generally limited to intertidal areas due to the difficulties and costs associated with working in subtidal areas. In this work, the suitability of using landscape mosaicing techniques is analyzed for the application of the "Quality of Rocky Bottoms" index (CFR by its Spanish acronym) in subtidal areas. For this purpose, the sensitivity and accuracy of both the indicators that compose the CFR index (characteristic macroalgae coverage, fraction of opportunistics and characteristic macroalgae richness) and the index itself were tested against different sampling surfaces and validated through direct applications of the CFR carried out in situ by scuba divers. The study was carried out at three sites, located on the coast of Cantabria (N. Spain), covering a variety of environmental conditions (depth ranges and anthropogenic pressures). Underwater video transects of 5-20m length were recorded by scuba divers and processed with specialized software to build continuous image mosaics of the assessment sites. Each mosaic was inserted into a Geographical Information System where all distinguishable macroalgal species were identified and their coverages were estimated. Replicated subsamples of different areas (0.25m2, 0.5m2, 1m2 and 2.5m2) were tested from each mosaic for the estimation of both the single indicators and the CFR index itself. Main results showed that larger subsample areas produced higher and more accurate CFR values, mainly related to higher srichness values and to smaller variability within the replicates. Accordingly, the minimum sample size required to carry out this type of studies was estimated to be of 2.5m2, showing no significant differences with the total mosaics. At this spatial scale, the assessments of the CFR index using mosaics showed a significant correlation and an excellent agreement with the results obtained in situ. In summary, underwater video mosaicing techniques proved to be a useful tool for the application of the CFR index and could also be of great interest for the study of subtidal environments by allowing visualization of extensive seafloor areas.},
author = {Guinda, Xabier and Gracia, Ana and Puente, Araceli and Juanes, Jos{\'{e}} Antonio and Rzhanov, Yuri and Mayer, Larry},
doi = {10.1016/j.dsr2.2013.09.037},
file = {:media/documentos/Drive USB/Publicaciones/2014/2014{\_}Guinda et al.{\_}Application of landscape mosaics for the assessment of subtidal macroalgae communities using the CFR index.pdf:pdf},
isbn = {0967-0645},
issn = {09670645},
journal = {Deep-Sea Research Part II: Topical Studies in Oceanography},
keywords = {CFR index,Landscape mosaics,Sample size,Subtidal macroalgae,Water Framework Directive},
month = {oct},
number = {2013},
pages = {207--215},
publisher = {Elsevier},
title = {{Application of landscape mosaics for the assessment of subtidal macroalgae communities using the CFR index}},
volume = {106},
year = {2014}
}
@article{Soriano2001,
abstract = {The purpose of this study is to automate coral reef assessment,$\backslash$nthat is, to classify coral images into benthic categories from digitized$\backslash$nunderwater video using a computer-based classifier such that coral reef$\backslash$nanalysis becomes less subjective, less tedious and more precise. Corals$\backslash$nexhibit a variety of color, texture and structure which are the visual$\backslash$ncues used by marine scientists for their classification. In computer$\backslash$nvision, color is a point property of a picture element while texture is$\backslash$na property of an area. Color and texture have been combined as$\backslash$ncolor-texture which is a feature that describes the spatial organization$\backslash$nof colors in an area. As inputs to a classifier, the authors extract$\backslash$ncolor, texture and color-texture descriptors from coral images and$\backslash$nmeasure recognition rates using each feature. Corals are 3D structures$\backslash$nand, when imaged, are prone to varying resolutions, perspective$\backslash$nprojection and lighting conditions. Therefore, an additional objective$\backslash$nof this study is to address the problem of illumination, rotation and$\backslash$nscale invariance in pattern recognition of underwater images. Images$\backslash$nwere classified into one of five benthic categories: alive coral, dead$\backslash$ncoral, dead coral with algae, algae and abiotics. Overall, texture was$\backslash$nfound to be more discriminating than using color alone or color and$\backslash$ntexture combined. Dead coral was the most successfully recognized class$\backslash$nusing color features},
annote = {From Duplicate 1 (Image classification of coral reef components from underwater color$\backslash$nvideo - Soriano, M; Marcos, Sheila; Saloma, C.; Quibilan, M.; Alino, P.)

Detalla proceso de clasificacion de organismos (alga, coral vivo, muerto, arena, extras) mediante imagenes extraidas de videos.
Usa LBP, como operador de binarizacion de imagen para detectar texturas en la imagen.
Procede a una transformacion de espacio (RGB-{\textgreater}NCC) y luego clasifica con KNN, usa como ejemplo de entrenamiento pocos casos.
Problemas clasificando corales vivos, dada la variedad en colores y texturas de la clase. Exitosa clasificacion de coral muerto, al igual que arena. Ambas clases comparte misma cromatticidad, solo cambia la intensidad (Y)},
