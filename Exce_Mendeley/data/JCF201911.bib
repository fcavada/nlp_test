@article{Leabourne2002,
abstract = {This paper presents research on the hydrodynamic modeling of a$\backslash$nmanipulator for an autonomous underwater scientific vehicle. The focus$\backslash$nis on improving the modeling accuracy of the in-line hydrodynamic$\backslash$ncoupling between a two-link manipulator and a small, free-floating$\backslash$nvehicle in order to achieve better control for coordinated motion of the$\backslash$ncombined system. Loads predicted using existing models for underwater$\backslash$narms were determined to be off by as much as 25{\%} when applied to a real,$\backslash$ntwo-link arm in a test tank. In this new approach, an$\backslash$nexperimentally-determined model has been developed that takes into$\backslash$naccount the 3D flow effects that have previously not been included. The$\backslash$nend result is a model that provides accurate predictions for the joint$\backslash$ntorques of a two-link arm in a form simple enough to be implemented in$\backslash$nalgorithms for precision planning and control. This project is part of a$\backslash$njoint program between the Aerospace Robotics Laboratory at Stanford$\backslash$nUniversity and the Monterey Bay Aquarium Research Institute},
