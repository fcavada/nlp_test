journal = {Methods in Ecology and Evolution},
keywords = {3D fractal dimensions,3D models,3D scanning,comparative shape analysis,multiscale fractal dimension,rugosity,scleractinian corals,shape and complexity quantification,surface–volume (S/V) ratio},
number = {12},
pages = {1650--1658},
