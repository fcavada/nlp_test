journal = {Journal of Experimental Biology},
keywords = {CPG,ichthyomyzon unicuspis,intersegmental coordination,intersegmental coupling to be,lamprey,movement,oscillator,petromyzon marinus,sensory feedback,spinal cord,strength and direction of},
mendeley-tags = {CPG},
month = {jul},
number = {13},
pages = {2361--2370},
pmid = {11507118},
title = {{Impact of movement and movement-related feedback on the lamprey central pattern generator for locomotion}},
url = {papers3://publication/uuid/5B6E5C27-538F-4066-864B-16C6AF3C255D},
volume = {204},
year = {2001}
}
