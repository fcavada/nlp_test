file = {:media/documentos/Drive USB/Publicaciones/2019/2019{\_}Neyer, Nocerino, Gruen{\_}IMAGE QUALITY IMPROVEMENTS in LOW-COST UNDERWATER PHOTOGRAMMETRY.pdf:pdf},
issn = {21949050},
journal = {ISPRS Annals of the Photogrammetry, Remote Sensing and Spatial Information Sciences},
keywords = {3D modelling,GoPro,Underwater photogrammetry,accuracy evaluation,action cameras,image quality},
number = {2/W10},
