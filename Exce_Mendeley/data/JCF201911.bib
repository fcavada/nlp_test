file = {:media/documentos/Drive USB/Publicaciones/2017/2017{\_}Buscombe{\_}Shallow water benthic imaging and substrate characterization using recreational-grade sidescan-sonar.pdf:pdf},
isbn = {1364-8152},
issn = {13648152},
journal = {Environmental Modelling and Software},
keywords = {Acoustical remote sensing,Benthic habitat,Habitat mapping,Sidescan sonar},
pages = {1--18},
publisher = {Elsevier Ltd},
title = {{Shallow water benthic imaging and substrate characterization using recreational-grade sidescan-sonar}},
