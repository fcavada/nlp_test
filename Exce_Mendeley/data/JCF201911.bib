file = {:media/documentos/Drive USB/Publicaciones/2003/2003{\_}Nakada, Asai, Amemiya{\_}An Analog Neural Oscillator Circuit for Locomotion Controller in Quadruped Walking Robot.pdf:pdf},
isbn = {1045-9227},
issn = {10459227},
journal = {IEEE Transactions on Neural Networks},
keywords = {Analog CMOS circuits,CPG,Central pattern generator (CPG),Coupled neural oscillators,Interlimb coordination,Quadruped locomotion},
