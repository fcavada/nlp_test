@inproceedings{Park2013,
abstract = {We propose a method for accurate 3D shape reconstruction using $\backslash$nuncalibrated multiview photometric stereo. A coarse mesh reconstructed using $\backslash$nmultiview stereo is first parameterized using a planar mesh parameterization $\backslash$ntechnique. Subsequently, multiview photometric stereo is performed in the 2D $\backslash$nparameter domain of the mesh, where all geometric and photometric cues from $\backslash$nmultiple images can be treated uniformly. Unlike traditional methods, there is $\backslash$nno need for merging view-dependent surface normal maps. Our key contribution is $\backslash$na new photometric stereo based mesh refinement technique that can efficiently $\backslash$nreconstruct meshes with extremely fine geometric details by directly estimating $\backslash$na displacement texture map in the 2D parameter domain. We demonstrate that $\backslash$nintricate surface geometry can be reconstructed using several challenging $\backslash$ndatasets containing surfaces with specular reflections, multiple albedos and $\backslash$ncomplex topologies.},
author = {Park, Jaesik and Sinha, Sudipta N. and Matsushita, Yasuyuki and Tai, Yu Wing and Kweon, In So},
booktitle = {Proceedings of the IEEE International Conference on Computer Vision},
doi = {10.1109/ICCV.2013.148},
file = {:media/documentos/Drive USB/Publicaciones/2013/2013{\_}Park et al.{\_}Multiview photometric stereo using planar mesh parameterization.pdf:pdf},
