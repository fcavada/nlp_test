file = {:media/documentos/Drive USB/Publicaciones/2005/2005{\_}Denker et al.{\_}A network of electronic neural oscillators reproduces the dynamics of the periodically forced pyloric pacemaker group.pdf:pdf},
issn = {00189294},
journal = {IEEE Transactions on Biomedical Engineering},
keywords = {CPG,Electronic neurons,Neural modeling,Nonlinear oscillators,Pacemaker group,Periodic forcing},
