keywords = {Exploration,Mapping,Remote Geology,Science Autonomy,Space Robotics},
title = {{Intelligent Maps for Autonomous Kilometer-Scale Science Survey}},
year = {2008}
}
@article{Best2017,
abstract = {We consider an optimal stopping formulation of the mission monitoring problem, in which a monitor vehicle must remain in close proximity to an autonomous robot that stochastically follows a predicted trajectory. This problem arises in a diverse range of scenarios, such as autonomous underwater vehicles supervised by surface vessels, pedestrians monitored by aerial vehicles, and animals monitored by agricultural robots. The key problem characteristics we consider are that the monitor must remain stationary while observing the robot, robot motion is modeled in general as a stochastic process, and observations are modeled as a spatial probability distribution. We propose a resolution-complete algorithm that runs in a polynomial time. The algorithm is based on a sweep-plane approach and generates a motion plan that maximizes the expected observation time and value. A variety of stochastic models may be used to represent the robot trajectory. We present results with data drawn from real AUV missions, a real pedestrian trajectory dataset and Monte Carlo simulations. Our results demonstrate the performance and behavior of our algorithm, and relevance to a variety of applications.},
author = {Best, Graeme and Martens, Wolfram and Fitch, Robert},
doi = {10.1109/TRO.2017.2653196},
file = {:media/documentos/Drive USB/Publicaciones/2017/2017{\_}Best, Martens, Fitch{\_}Path planning with spatiotemporal optimal stopping for stochastic mission monitoring.pdf:pdf},
issn = {15523098},
journal = {IEEE Transactions on Robotics},
keywords = {Agricultural robotics,Combinatorial optimization,Marine robotics,Mission monitoring,Optimal stopping,Path planning,Pedestrian tracking,Planning under uncertainty},
number = {3},
pages = {629--646},
publisher = {IEEE},
title = {{Path planning with spatiotemporal optimal stopping for stochastic mission monitoring}},
volume = {33},
year = {2017}
}
@article{Ma2018,
abstract = {Robots such as autonomous underwater vehicles (AUVs) and autonomous surface vehicles (ASVs) have been used for sensing and monitoring aquatic environments such as oceans and lakes. Environmental sampling is a challenging task because the environmental attributes to be observed can vary both spatially and temporally, and the target environment is usually a large and continuous domain whereas the sampling data is typically sparse and limited. The challenges require that the sampling method must be informative and efficient enough to catch up with the environmental dynamics. In this paper we present a planning and learning method that enables a sampling robot to perform persistent monitoring tasks by learning and refining a dynamic "data map" that models a spatiotemporal environment attribute such as ocean salinity content. Our environmental sampling framework consists of two components: to maximize the information collected, we propose an informative planning component that efficiently generates sampling waypoints that contain the maximal information; To alleviate the computational bottleneck caused by large-scale data accumulated, we develop a component based on a sparse Gaussian Process whose hyperparameters are learned online by taking advantage of only a subset of data that provides the greatest contribution. We validate our method with both simulations running on real ocean data and field trials with an ASV in a lake environment. Our experiments show that the proposed framework is both accurate in learning the environmental data map and efficient in catching up with the dynamic environmental changes.},
author = {Ma, Kai Chieh and Liu, Lantao and Heidarsson, Hordur K. and Sukhatme, Gaurav S.},
doi = {10.1002/rob.21767},
file = {:media/documentos/Drive USB/Publicaciones/2018/2018{\_}Ma et al.{\_}Data-driven learning and planning for environmental sampling.pdf:pdf},
issn = {15564967},
journal = {Journal of Field Robotics},
keywords = {Gaussian process,environmental monitoring,informative planning,learning,marine robotics},
number = {5},
pages = {643--661},
title = {{Data-driven learning and planning for environmental sampling}},
volume = {35},
year = {2018}
}
