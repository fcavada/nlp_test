@article{Purkis2012,
abstract = {The search for, and extraction of, hydrocarbons in carbonate rocks$\backslash$ndemands a thorough understanding of their depositional anatomy. The$\backslash$ncomplexity of carbonate systems, however, hinders detailed direct$\backslash$ncharacterization of their volumetric heterogeneity. Information with$\backslash$nwhich to construct a reservoir model must therefore be based on$\backslash$ninformation gathered from wells or outcrops transecting the sequence of$\backslash$ninterest. Most (particularly exploration wells) are vertical, presenting$\backslash$na problem for geostatistical modeling. While understanding vertical$\backslash$nstratal stacking is straightforward, it is difficult to obtain lateral$\backslash$nfacies information. Though in some situations outcrop surfaces, seismic$\backslash$ndata, and horizontal wells may somewhat mitigate this bias, the$\backslash$nlikelihood remains that the lateral dimension of a buried system will be$\backslash$nvastly undersampled with respect to the vertical. However, through the$\backslash$nprinciple of Walther's Law (Walther 1894) or due to the geometry of$\backslash$nbasinward-inclined beds, comparable facies frequencies and transition$\backslash$nprobabilities may link vertical and lateral stratal arrangements, the$\backslash$nimplication being that a reservoir model, competent at least in terms of$\backslash$ntransition statistics, could be built against information harvested$\backslash$ndown-core. Taking an interpreted outcrop panel from Lewis Canyon$\backslash$n(Albian, Pecos River, Texas), we use Markov-chains to first ascertain$\backslash$nthat vertical and lateral stratal ordering is nonrandom. Second, we show$\backslash$nlithofacies transition probabilities in the outcrop as being$\backslash$ninterchangeable between the vertical and lateral directions. The work$\backslash$nconcludes by demonstrating the utility of an existing 3-D Markov random$\backslash$nfield simulation to volumetrically model the Lewis Canyon outcrop on the$\backslash$nbasis of vertical facies transition tendencies. Statistical$\backslash$ninterrogation of the 3-D model output reveals the simulation to contain$\backslash$nrealistic facies associations compared to the outcrop. This suggests$\backslash$nthat the reconstruction process, based on Markov chains, produces a$\backslash$nuseful representation of 3-D heterogeneity in this Lower Cretaceous$\backslash$ncarbonate succession. Markov random field simulation might provide an$\backslash$nimportant tool for prediction and simulation of subsurface carbonate$\backslash$nreservoirs.},
author = {Purkis, S. and Vlaswinkel, B. and Gracias, Nuno Ricardo},
doi = {10.2110/jsr.2012.23},
file = {:media/documentos/Drive USB/Publicaciones/2012/2012{\_}Purkis, Vlaswinkel, Gracias{\_}Vertical-To-Lateral Transitions Among Cretaceous Carbonate Facies--A Means To 3-D Framework Constructio.pdf:pdf},
isbn = {1527-1404 Unique ID: WOS:000304287200007},
issn = {1527-1404},
journal = {Journal of Sedimentary Research},
keywords = {Society for Sedimentary Geology},
month = {may},
number = {4},
pages = {232--243},
title = {{Vertical-To-Lateral Transitions Among Cretaceous Carbonate Facies--A Means To 3-D Framework Construction Via Markov Analysis}},
