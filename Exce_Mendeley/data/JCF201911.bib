isbn = {1296-2074},
issn = {12962074},
journal = {Journal of Cultural Heritage},
keywords = {3D data comparison,3D range scanning,Dense multi-view 3D reconstruction,Structure-From-Motion,Total station survey,Unmanned aerial vehicle},
month = {jan},
number = {1},
pages = {73--79},
